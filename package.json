{
  "name": "try-read-json",
  "version": "1.0.0",
  "description": "Graceful reading of JSON value, using JSON.parse with support for optional callback",
  "repository": "tunnckoCore/try-read-json",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://www.tunnckocore.tk)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard --verbose",
    "pretest": "npm run lint",
    "test": "npm run coverage",
    "posttest": "npm run lint:coverage",
    "coverage": "nyc node test.js",
    "lint:coverage": "nyc check-coverage --lines 100 --branches 100 --statements 100 --functions 100",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "precommit": "git add --all",
    "commit": "git-cz"
  },
  "dependencies": {
    "try-catch-callback": "^2.0.1"
  },
  "devDependencies": {
    "commitizen": "^2.8.6",
    "coveralls": "^2.11.14",
    "cz-conventional-changelog": "^1.2.0",
    "mukla": "^0.4.7",
    "nyc": "^8.3.2",
    "pre-commit": "^1.1.3",
    "standard": "^8.5.0",
    "standard-version": "^3.0.0"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "catch",
    "correct",
    "data",
    "fast",
    "flow",
    "json",
    "meaningful",
    "read",
    "read-data",
    "read-json",
    "throws",
    "try",
    "try-catch",
    "trycatch"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "verb": {
    "run": true,
    "toc": true,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "try-catch-callback",
        "try-catch-core",
        "always-done",
        "then-parse-json",
        "write-file",
        "to-callback"
      ]
    },
    "reflinks": [
      "always-done",
      "async-done",
      "dezalgo",
      "once",
      "rfpify",
      "try-catch-core"
    ],
    "lint": {
      "reflinks": true
    }
  }
}